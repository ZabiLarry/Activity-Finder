<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="246.0" prefWidth="246.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.LogInController">
   <children>
      <Button fx:id="txtLogin" layoutX="130.0" layoutY="154.0" mnemonicParsing="false" onAction="#loginCommercial" prefHeight="30.0" prefWidth="94.0" text="Commercial">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextField fx:id="emailTF" layoutX="19.0" layoutY="54.0" prefHeight="38.0" prefWidth="204.0" promptText="Email">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordTF" layoutX="19.0" layoutY="104.0" prefHeight="38.0" prefWidth="204.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" layoutX="19.0" layoutY="14.0" prefHeight="27.0" prefWidth="150.0" text="Sign In">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="67.0" layoutY="198.0" mnemonicParsing="false" onAction="#forgotPass" text="Forgot password?" />
      <Button fx:id="txtLogin1" layoutX="20.0" layoutY="154.0" mnemonicParsing="false" onAction="#loginRegular" prefHeight="30.0" prefWidth="94.0" text="Regular">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="regBtn" layoutX="169.0" layoutY="15.0" mnemonicParsing="false" text="Register" />
   </children>
</AnchorPane>
